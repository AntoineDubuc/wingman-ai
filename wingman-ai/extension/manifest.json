{
  "manifest_version": 3,
  "key": "",
  "name": "Wingman - Real-Time AI Assistant",
  "version": "1.0.0",
  "description": "Your AI co-pilot for Google Meet. Get real-time suggestions, talking points, and smart responses during video calls.",
  "permissions": [
    "tabCapture",
    "activeTab",
    "storage",
    "offscreen",
    "scripting",
    "identity"
  ],
  "host_permissions": [
    "https://meet.google.com/*",
    "https://www.googleapis.com/*",
    "https://api.deepgram.com/*",
    "wss://api.deepgram.com/*",
    "https://generativelanguage.googleapis.com/*"
  ],
  "oauth2": {
    "client_id": "617701449574-b88mqgktdiqdgc3qdhe57mbqdm6oiq5a.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive.file",
      "https://www.googleapis.com/auth/userinfo.email"
    ]
  },
  "background": {
    "service_worker": "src/background/service-worker.ts",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "js": ["src/content/content-script.ts"],
      "css": ["src/content/content-styles.css"],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_popup": "src/popup/popup.html",
    "default_icon": {
      "16": "src/icons/icon-16.png",
      "48": "src/icons/icon-48.png",
      "128": "src/icons/icon-128.png"
    },
    "default_title": "Wingman AI"
  },
  "icons": {
    "16": "src/icons/icon-16.png",
    "48": "src/icons/icon-48.png",
    "128": "src/icons/icon-128.png"
  },
  "options_page": "src/options/options.html",
  "web_accessible_resources": [
    {
      "resources": ["src/content/audio-processor.worklet.js"],
      "matches": ["https://meet.google.com/*"]
    }
  ],
  "minimum_chrome_version": "116"
}
